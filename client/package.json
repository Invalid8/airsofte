{
  "name": "airsofte",
  "version": "1.1.3",
  "description": "A retro shooter game",
  "main": "./out/main/index.js",
  "author": "dalgoridim <b.fadamitan2019@gmail.com> (https://dantolu33.vercel.app)",
  "keywords": [
    "Game",
    "Retro Shooter",
    "Fighter Jets"
  ],
  "homepage": "https://airsofte.vercel.app",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "start:web": "vite dev --config vite.web.config.mts",
    "build:web": "vite build --config vite.web.config.mts",
    "preview:web": "vite preview --config vite.web.config.mts",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "build": {
    "asar": true,
    "files": [
      "out/**",
      "!**/*.map",
      "!**/*.ts",
      "!**/*.md",
      "!**/.git/**",
      "!**/.vscode/**",
      "!**/.svelte-kit/**",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/__tests__/**",
      "!**/.cache/**",
      "!**/.npm/**",
      "!**/.pnpm-store/**",
      "!**/docs/**",
      "!**/examples/**"
    ],
    "linux": {
      "category": "Game",
      "target": [
        "AppImage",
        "snap",
        "deb"
      ],
      "icon": "resources/icons"
    },
    "win": {
      "icon": "./resources/icon.ico"
    },
    "mac": {
      "icon": "./resources/icon.icns"
    },
    "afterPack": "scripts/prune-locales.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@google/generative-ai": "^0.24.1",
    "@vercel/analytics": "^1.6.1",
    "electron-updater": "^6.3.9",
    "gsap": "^3.13.0",
    "howler": "^2.2.4",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.5",
    "@types/howler": "^2.2.12",
    "@types/node": "^22.14.1",
    "autoprefixer": "^10.4.21",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-svelte": "^3.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.26.1",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.2.0"
  }
}
